{
    "$schema": "../../data/config.json",
    "name": "MyGame",
    "environment": {
        "xlsx": "./xlsx",
        "out": "./json",
        "localize": "./localization",
        "jsonschema": "./jsonschema"
     },
    "localization": {
        "baselanguage": "kr"
    },
    "xlsxtables": {
        "Items.xlsx": {
            "workSheets": [
                {
                    "name": "Equipment",
                    "out": "Items_Equipment.json",
                    "localize": {
                        "keycolumn": "/Key"
                    }
                },
                {
                    "name": "Consumable",
                    "out": "Items_Consumable.json",
                    "kwargs": {
                        "start_line": 2
                    }                    
                }
            ]
        },
        "TestData.xlsx": {
            "workSheets": [
                {
                    "name": "ColumnOrient",
                    "out": "TestData_Column.json",
                    "kwargs": {
                        "start_line": 3,
                        "row_oriented": false
                    }
                },
                {
                    "name": "Csv",
                    "out": "TestData_Csv.csv",
                    "localize": {
                        "keycolumn": "/Key"
                    }
                },
                {
                    "name": "Array",
                    "out": "TestData_Array.json"
                },
                {
                    "name": "Object",
                    "out": "TestData_Object.json"
                }
            ]
        },
        "Folder/Character.xlsx": {
            "workSheets": [
                {
                    "name": "Data",
                    "out": "Character.json",
                    "localize": {
                        "keycolumn": "/Key"
                    }
                }
            ]
        },
        "NotExistFile.xlsm": {

        }
    }
}